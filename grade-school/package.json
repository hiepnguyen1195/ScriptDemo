{
  "name": "grade-school",
  "version": "1.0.0",
  "main": "grade-school.ts",
  "author": "hiepnn",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "jest": "^21.2.1",
    "ts-jest": "^22.0.1",
    "typescript": "^2.6.2"
  },
  "scripts": {
    "test": "tsc --noEmit -p . && jest --no-cache",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
